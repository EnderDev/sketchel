        <script src="/js/canvas.js"></script>
        <link href="/css/canvas.css" rel="stylesheet">
    </head>
    <body>
        <div class="SketchelApp">
            <div class="SketchelCanvasWrapper">
                <canvas class="SketchelCanvas">
                    Your browser doesn't support canvas... Are you using the first version of Internet Explorer or something??
                </canvas>
            </div>
            <div class="UndoBtn"></div>
            <div class="RedoBtn"></div>
            <div class="ClearBtn"></div>
            <div class="PencilBtn"></div>
            <div class="EraserBtn"></div>
            <div class="ExportBtn"></div>
            <div class="BucketBtn"></div>
            <div class="BrushSliderWrapper">
                <input class="Brushslider" type="range" min="1" max="100" value="10">
                <p class="BrushSizeParagraph">Brush Size: <span class="BrushSize">10</span></p>
            </div>
            <div class="pickr"></div>
        </div>
    </body>
